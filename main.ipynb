    "import cv2\n",
    "import numpy as np\n",
    "import skfuzzy  as fuzz\n",
    "from skfuzzy import control as ctrl\n",
    "\n",
    "# Load the image\n",
    "image_path = 'path/to/your/image.jpg'\n",
    "image = cv2.imread(image_path, cv2.IMREAD_GRAYSCALE)\n",
    "\n",
    "# Fuzzy system design\n",
    "brightness = ctrl.Antecedent(np.arange(0, 256, 1), 'brightness')\n",
    "output_brightness = ctrl.Consequent(np.arange(0, 256, 1), 'output_brightness')\n",
    "\n",
    "# Define fuzzy sets and membership functions\n",
    "brightness['low'] = fuzz.trimf(brightness.universe, [0, 50, 100])\n",
    "brightness['medium'] = fuzz.trimf(brightness.universe, [50, 100, 150])\n",
    "brightness['high'] = fuzz.trimf(brightness.universe, [100, 150, 255])\n",
    "\n",
    "output_brightness['low'] = fuzz.trimf(output_brightness.universe, [0, 50, 100])\n",
    "output_brightness['medium'] = fuzz.trimf(output_brightness.universe, [50, 100, 150])\n",
    "output_brightness['high'] = fuzz.trimf(output_brightness.universe, [100, 150, 255])\n",
    "\n",
    "# Define rules\n",
    "rule1 = ctrl.Rule(brightness['low'], output_brightness['low'])\n",
    "rule2 = ctrl.Rule(brightness['medium'], output_brightness['medium'])\n",
    "rule3 = ctrl.Rule(brightness['high'], output_brightness['high'])\n",
    "\n",
    "# Create a control system\n",
    "brightness_ctrl = ctrl.ControlSystem([rule1, rule2, rule3])\n",
    "brightness_adjustment = ctrl.ControlSystemSimulation(brightness_ctrl)\n",
    "\n",
    "# Apply fuzzy logic to the image\n",
    "adjusted_image = np.zeros_like(image)\n",
    "for i in range(image.shape[0]):\n",
    "    for j in range(image.shape[1]):\n",
    "        brightness_adjustment.input['brightness'] = image[i, j]\n",
    "        brightness_adjustment.compute()\n",
    "        adjusted_image[i, j] = brightness_adjustment.output['output_brightness']\n",
    "\n",
    "# Display the original and adjusted images\n",
    "cv2.imshow('Original Image', image)\n",
    "cv2.imshow('Adjusted Image', adjusted_image.astype(np.uint8))\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
