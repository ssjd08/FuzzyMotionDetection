    "#v4\n",
    "import cv2\n",
    "import numpy as np\n",
    "\n",
    "# 1. Capture background every 10 minutes.\n",
    "# 2. Set a threshold for the size of moving obj.\n",
    "# 3. Set a threshold for the displacement obj.\n",
    "\n",
    "cap = cv2.VideoCapture(0)\n",
    "fps = int(cap.get(cv2.CAP_PROP_FPS))\n",
    "sec = 5\n",
    "\n",
    "\n",
    "def get_background():\n",
    "# Capture initial background frames\n",
    "    background_frames = []  \n",
    "    for i in range(sec * fps):\n",
    "        ret, frame = cap.read()\n",
    "        background_frames.append(frame)\n",
    "        if not ret:\n",
    "            print(\"Failed to read frame\")\n",
    "            exit()\n",
    "\n",
    "    # Initialize background image and grayscale background\n",
    "    background = np.average(background_frames, axis=0).astype(np.uint8)\n",
    "    gray_background = cv2.cvtColor(background, cv2.COLOR_BGR2GRAY)\n",
    "    return gray_background\n",
    "\n",
    "# Threshold value for detecting motion\n",
    "threshold = 50\n",
    "\n",
    "\n",
    "kernel = np.ones((5, 5))/25\n",
    "\n",
    "frame_count = 0\n",
    "while cap.isOpened():\n",
    "    ret, frame = cap.read()\n",
    "\n",
    "    # Update background every 30 minutes (1800 frames)\n",
    "    if frame_count % 1800 == 0:\n",
    "        gray_background = get_background()\n",
    "        \n",
    "    # Calculate frame difference\n",
    "    gray_frame = cv2.cvtColor(frame, cv2.COLOR_BGR2GRAY)\n",
    "    frame_diff = cv2.absdiff(gray_frame, gray_background)\n",
    "    cv2.imshow('frame_diff',frame_diff)\n",
    "\n",
    "    # Apply threshold and dilation\n",
    "    ret, thres = cv2.threshold(frame_diff, threshold, 255, cv2.THRESH_BINARY)\n",
    "    # thres = cv2.dilate(thres, kernel, iterations=5)\n",
    "\n",
    "    # Display binary image\n",
    "    cv2.imshow(\"binary_frame\", thres)\n",
    "    # print(\"binary_frame\")\n",
    "\n",
    "    # Check for termination\n",
    "    if cv2.waitKey(40) == 27 or not ret :\n",
    "        break\n",
    "\n",
    "    frame_count += 1\n",
    "\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()\n",
    "cap.release()\n"
   ]
